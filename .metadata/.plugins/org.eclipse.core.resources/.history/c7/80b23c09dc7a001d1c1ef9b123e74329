package com.deloitte.repo;

import java.util.List;

import org.springframework.data.jpa.repository.JpaRepository;
import org.springframework.data.jpa.repository.Query;
import org.springframework.stereotype.Repository;

import com.deloitte.entity.Food;

@Repository
public interface IFoodRepo extends JpaRepository<Food, String> {
	public List<Food> findByFoodIsJain(Boolean foodIsJain);
	@Query("select * from food where food_price <100")
	public List<Food> findFoodUnderX(Double foodPrice);
}
